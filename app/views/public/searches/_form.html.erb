<% if user_signed_in? %>
  <%= form_with url: search_path, method: :get, local: true do |f| %>
    <%= f.label :レビュー検索 %>
    <%= f.text_field :content, placeholder: "キーワードを入力" %>
    <%= f.hidden_field :model, value: "zoo_review" %>
    <%= f.hidden_field :method, value: "partial" %>
    <%= f.submit '検索' %>
  <% end %>
<% end %>